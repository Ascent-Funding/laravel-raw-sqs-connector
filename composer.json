{
  "name": "ascent-funding/laravel-raw-sqs-connector",
  "description": "Allows for the consumption of raw messages produced outside of Laravel from SQS to then be handled natively within Laravel's Queue and Job system",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Harnasz",
      "email": "tom@primitivesense.com"
    },
    {
      "name": "Ascent Funding",
      "email": "appdev@ascentfunding.com"
    }
  ],
  "require": {
    "php": ">=7.3 || ^8.0",
    "ext-json": "*",
    "illuminate/queue": "^8.0 | ^9.0 | ^10.0",
    "aws/aws-sdk-php": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "PrimitiveSense\\LaravelRawSqsConnector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-mockery": "^0.12.0",
    "squizlabs/php_codesniffer": "^3.4",
    "mockery/mockery": "^1.2"
  }
}
